{
    "compilerOptions": {
        "target": "ES2019",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "dist"
    },
    "exclude": [
        "compiled/*",
        "node_modules/*"
    ],
    "files": ["serve.ts", "dataStore.ts"],
    "baseUrl": "types",
    "typeRoots": ["types"]
}
